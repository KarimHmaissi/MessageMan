<div class="row">
	<div class="small-12 columns">
		<h2>Create a new message</h2>
		<div><a href="/message">Your messages</a></div>
	</div>

	<div class="small-6 columns end">
		<form role="form" action="/message/create" method="post">
		  <div class="form-group">
		    <label for="url">Url</label>
		    <input type="text" class="form-control" id="url" name="url" placeholder="URL">
		  </div>
		  <div class="form-group">
		    <label for="content">content</label>
		    <input type="text" class="form-control" id="content" name="content" placeholder="content">
		  </div>
		  <div class="form-group">
		    <label for="position">position</label>
		    <input type="text" class="form-control" id="position" name="position" placeholder="position">
		  </div>
		  <div class="form-group">
		    <label for="multistep">multistep</label>
		    <input type="text" class="form-control" id="multistep" name="multistep" placeholder="multistep">
		  </div>
		  <div class="form-group">
		    <label for="multiStepContent">multiStepContent</label>
		    <input type="text" class="form-control" id="multiStepContent" name="multiStepContent" placeholder="multiStepContent">
		  </div>
		  <div class="form-group">
		    <label for="hook">hook</label>
		    <input type="text" class="form-control" id="hook" name="hook" placeholder="hook">
		  </div>

		  <button type="submit" class="btn btn-default">Submit</button>
		</form>
	</div>
</div>
